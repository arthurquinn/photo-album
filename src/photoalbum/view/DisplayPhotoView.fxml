<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1" fx:controller="photoalbum.view.DisplayPhotoViewController">
   <padding>
      <Insets bottom="3.0" left="3.0" right="3.0" top="3.0" />
   </padding>
   <children>
      <ImageView fx:id="imgView" fitHeight="497.0" fitWidth="792.0" layoutX="10.0" layoutY="10.0" pickOnBounds="true" preserveRatio="true" />
      <Label fx:id="lblCaption" layoutX="14.0" layoutY="507.0" text="Caption: " />
      <Label fx:id="lblCaptureDateTime" layoutX="14.0" layoutY="524.0" text="Date/Time of Capture:" />
      <Label fx:id="lblTags" layoutX="14.0" layoutY="541.0" text="Tags:" />
      <Button fx:id="btnClose" alignment="TOP_LEFT" layoutX="761.0" layoutY="579.0" mnemonicParsing="false" nodeOrientation="LEFT_TO_RIGHT" text="Close" />
   </children>
</AnchorPane>
